#!/bin/sh

RPM_SOURCE_DIR="/home/rpm/distribute/SOURCES"
RPM_BUILD_DIR="/home/rpm/distribute/BUILD"
RPM_OPT_FLAGS="-O2 -g -pipe -Wall -Wp,-D_FORTIFY_SOURCE=2 -fexceptions -fstack-protector --param=ssp-buffer-size=4 -m64 -mtune=generic"
RPM_ARCH="x86_64"
RPM_OS="linux"
export RPM_SOURCE_DIR RPM_BUILD_DIR RPM_OPT_FLAGS RPM_ARCH RPM_OS
RPM_DOC_DIR="/usr/share/doc"
export RPM_DOC_DIR
RPM_PACKAGE_NAME="elastix-python2.7-distribute"
RPM_PACKAGE_VERSION="0.6.49"
RPM_PACKAGE_RELEASE="1"
export RPM_PACKAGE_NAME RPM_PACKAGE_VERSION RPM_PACKAGE_RELEASE
RPM_BUILD_ROOT="/home/rpm/elastix-python2.7-distribute-0.6.49-root"
export RPM_BUILD_ROOT

PKG_CONFIG_PATH="/usr/lib64/pkgconfig:/usr/share/pkgconfig"
export PKG_CONFIG_PATH

set -x
umask 022
cd /home/rpm/distribute/BUILD

cd 'distribute-0.6.49'
LANG=C
export LANG
unset DISPLAY

rm -rf $RPM_BUILD_ROOT
mkdir -p /home/rpm/elastix-python2.7-distribute-0.6.49-root/lib/python2.7/site-packages/
python2.7 setup.py install --prefix=$RPM_BUILD_ROOT/usr/local


    
    
    
